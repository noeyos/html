<script>
const myCar = {
	color: 'red',
	name : 'bmw',
	speed : 100,
	
	brake : function() {
		this.speed = this.speed + 10;
	},
	accel : function() {
		this.speed -= 10;
	}
	
}


function proc1() {
	printObj('result1')
	
}

function printObj(result) {

	// 출력 내용
	str = `이름: ${myCar.name} <br>`
	str += `색상: ${myCar.color} <br>`
	
	
	
	
	document.getElemendById(result)
}

</script>
