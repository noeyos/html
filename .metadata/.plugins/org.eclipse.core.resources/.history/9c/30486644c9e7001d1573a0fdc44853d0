<script>
function proc1() {
	let today = new Date();
	
	let day1 = today.toLocaleString();
	
	// 출력 내용
	let str = "오늘은 " + today + "<br>";
	str += "우리는 " + day1 + "<br>";
	
}

</script>
<body>
<input type="button" value="확인" onclick="proc1()">
</body>
