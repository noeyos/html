today1 = new Date();
today2 = new Date(100000);
today3 = new Date("1999.07.25");
today4 = new Date(1999, 07-1, 25);

proc1 = () => {
	
	str = today1 + "<br>";
	str += today2 + "<br>";
	str += today3 + "<br>";
	str += today4 + "<br>";
	
	document.getElementById('result1').innerHTML = str;
}

</script>

