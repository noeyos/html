<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<link rel="stylesheet" href="../css/mystyle.css">
<script>
// 속성 
const rect = {
	garo : 5,
	sero : 3,
	
	getArea() {
		return this.garo * this.sero;
	},
	
	getCircum() {
		return (parseInt(this.garo) + parseInt(this.sero)) * 2;
	}
		
}

function proc1() {
	 
	
	// 출력
	str = `가로: ${rect.garo} <br>`;
	str += `세로: ${rect.sero} <br>`;
	
	/*
	area = rect.getArea();
	circum = rect.getCircum();
	str += `넓이: ${area} <br>`;
	str += `둘레: ${circum} <br>`;
	*/

	str += `면적: ${rect.getArea()} <br>`;
	str += `둘레: ${rect.getCircum()} <br>`;
	
	document.getElementById('result1').innerHTML = str;
	
}
</script>
</head>
<body>
	<div class="box">
		<h3>객체의 속성과 메소드를 동적으로 추가</h3>
		<h3>const rect = { }</h3>
		<input type="button" value="확인" onclick="proc1()">
		<div id="result1" class="result"></div>
	</div>
</body>
</html>